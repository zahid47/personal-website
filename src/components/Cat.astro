---
import { Image } from "@astrojs/image/components";

const cat = {
  src: "/images/cats/cat_1.svg",
  alt: "A cute line-art illustration of a cat wearing a suit",
};
---

<Image
  class="self-center m-6"
  src={cat.src}
  alt={cat.alt}
  format="svg"
  height={120}
  width={120}
  aspectRatio={1}
/>
